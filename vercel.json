{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://placement-preparation-tracker.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://placement-preparation-tracker.onrender.com/api/auth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
